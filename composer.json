{
    "name": "sallycms/be-search",
    "description": "AddOn for SallyCMS, providing enhancements for the backend UI in structure and mediapool views",
    "authors": [
        {
            "name": "webvariants GbR"
        }
    ],
    "homepage": "http://www.sallycms.de/",
    "license": "MIT",
    "type": "sallycms-addon",
    "require": {
        "sallycms/composer-installer": "~1.1",
        "sallycms/sallycms": ">=0.7,<0.9"
    },
    "autoload": {
        "psr-0": {
            "besearch_": "lib/",
            "sly_Controller_": "lib/"
        }
    },
    "extra": {
        "sallycms": {
            "version": "1.7.x-dev",
            "imex-ignore": true
        },
        "branch-alias": {
            "dev-default": "1.x-dev"
        }
    }
}
